select t.*, t.rowid from TS_SP_INFO t order by ts_sp_cdate desc;
select t.*, t.rowid from TDY_TS_SP_INFO t
WHERE T.TDY_ZID='DB1212074D59962AEAA164DBE050007F01007F08';
select t.*, t.rowid from TS_SP_INFO t
WHERE T.TS_SP_INFO_ID='DB1212074D59962AEAA164DBE050007F01007F08';
--order by ts_sp_cdate desc;

select * from TS_SP_HS T
WHERE T.TS_SP_ID='DB1212074D59962AEAA164DBE050007F01007F08';



select SG_BSZY,SG_YWLX,SG_SBDW,SG_SBHH,SG_WZXH,SG_GBXH,DJSBDW,CARGOPLACE,SG_DSZTBAZT,BUSREGION,SG_SYBZGB,SG_SBGQ,SG_HGZT,SG_GJZT,SG_SPNBHH,SG_DYJLDW,SG_GJBZ,SG_SPSLGJ,SG_BADJ,SG_DEJLDW,SG_ZJYCPBZ,SG_SPBABH,SG_SPWZID,SG_XZLSPBZ,SG_ZDSJBJ,SG_SPDWGJ,SG_SCCJ,SG_SFJZJJBS,SG_3CZS
from TDY_TS_SP_INFO t
