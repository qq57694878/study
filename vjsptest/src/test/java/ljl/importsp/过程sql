select T1.TABLE_NAME,T1.COLUMN_NAME,T1.DATA_TYPE from user_tab_columns t1 where t1.TABLE_NAME in('TS_SP_INFO','TDY_TS_SP_INFO')
AND T1.COLUMN_NAME IN ('SG_YWLX','SG_SBDW','SG_SBHH','SG_WZXH','SG_PPBAM','SG_GBXH','DJSBDW','CARGOPLACE','SG_BANAME','SG_DSZTBAZT','BUSREGION','SG_SYBZGB','SG_SBGQ','SG_HGZT','SG_GJZT','SG_SPNBHH','SG_DYJLDW','SG_GJBZ','SG_SPSLGJ','SG_BADJ','SG_DEJLDW','SG_ZJYCPBZ','SG_SPBABH','SG_SPWZID','SG_XZLSPBZ','SG_ZDSJBJ','SG_SPDWGJ','SG_SCCJ','SG_SFJZJJBS','SG_3CZS');


