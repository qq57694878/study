/*==============================================================*/
/* Table: CUSTOMER                                              */
/*==============================================================*/
create table CUSTOMER
(
   CUST_ID              NUMBER(16)           not null,
   CREDIT_ID            NUMBER(16)           not null,
   SERVICE_GRADE_ID     NUMBER(16)           not null,
   CREDIT_LIMIT_ID      NUMBER(16)           not null,
   %TABLE%_PK           NUMBER(16)           not null,
   %TABLE%_PK2          NUMBER(16)           not null,
   PARTY_ID             NUMBER(16)           not null,
   TAX_PAYER_ID         NUMBER(16)           not null,
   CUST_NAME            VARCHAR2(250),
   CUST_NUMBER          VARCHAR2(30),
   CUST_ADDRESS         VARCHAR2(250),
   COMMON_REGION_ID     NUMBER(16)           not null,
   CUST_TYPE            VARCHAR2(10),
   CUST_SUB_TYPE        VARCHAR2(10),
   CUST_SOURCE_ID       NUMBER(16),
   CUST_AREA_GRADE      VARCHAR2(10),
   ENTER_DATE           DATE,
   CUST_STATUS_CD       VARCHAR2(10),
   CUST_STATUS_DATE     DATE,
   INDUSTRY_TYPE_ID     NUMBER(16)           not null,
   EXT_CUST_ID          NUMBER(9),
   DISTRIBUTOR_ID       NUMBER(9),
   SECRECY_LEV          VARCHAR2(30),
   CUST_ORDER_ID        NUMBER(16)           default -1 not null,
   CUST_CONTROL_LEVEL   VARCHAR2(10),
   IS_INSTEAD           NUMBER(2)
      constraint CKC_IS_INSTEAD_CUSTOMER check (IS_INSTEAD is null or (IS_INSTEAD in (0,1))),
   IS_REALNAME          NUMBER(2)
      constraint CKC_IS_REALNAME_CUSTOMER check (IS_REALNAME is null or (IS_REALNAME in (0,1))),
   STATUS_CD            VARCHAR2(10),
   STATUS_DATE          DATE,
   CREATE_STAFF         NUMBER(16),
   CREATE_DATE          DATE,
   UPDATE_STAFF         NUMBER(16),
   UPDATE_DATE          DATE,
   LAST_ORDER_ITEM_ID   NUMBER(16),
   REMARK               VARCHAR2(2000),
   constraint PK_CUSTOMER primary key (CUST_ID)
)

